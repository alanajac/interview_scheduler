from Schedules c 
inner join Users u on c.user_id = u.id and u.id = "1582721e-625c-4f70-97cc-9eb17a2810ff"
inner join Schedules i on c.slots = i.slots and i.roles = "interviewer" 
inner join Users u2 on u2.id = i.user_id;




